package com.criticalfusion.thenandnow.view{	import flash.display.Sprite;	public class Era2Scene extends EraScene	{		public var Era1_01p:Sprite;		public var Era1_02p:Sprite;		public var Era1_03p:Sprite;		public var Era1_04p:Sprite;		public var Era1_05p:Sprite;		public var Era1_06p:Sprite;		public var Era1_07p:Sprite;		public var Era1_08p:Sprite;		public var Era1_09p:Sprite;		public var Era1_10p:Sprite;		public var Era1_11p:Sprite;				public var Era2_02p:Sprite;		public var Era2_03p:Sprite;		public var Era2_04p:Sprite;		public var Era2_06p:Sprite;		public var Era2_08p:Sprite;		public var Era2_10p:Sprite;		public function Era2Scene()		{			super();			initialize();		}				final override public function showCompleted():void		{			Era1_02p.visible = false;			Era1_04p.visible = false;			Era1_10p.x = 270;			Era1_10p.y = -216;						Era2_02p.visible = true;			Era2_03p.visible = true;			Era2_04p.visible = true;			Era2_06p.visible = true;			Era2_08p.visible = true;			Era2_10p.visible = true;		}				final override public function checklist(value:String):void		{			switch(value)			{				case "Era2_02p":					hideItem("Era1_02p");					break;				case "Era2_04p":					hideItem("Era1_04p");					break;				case "Era2_06p":					Era1_10p.x = 270;					Era1_10p.y = -216;					break;			}		}				final protected function initialize():void		{			Era2_02p.visible = false;			Era2_03p.visible = false;			Era2_04p.visible = false;			Era2_06p.visible = false;			Era2_08p.visible = false;			Era2_10p.visible = false;		}	}}